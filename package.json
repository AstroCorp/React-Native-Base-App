{
  "name": "rnb",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "rn-link": "react-native link",
    "apk": "cd android && ./gradlew assembleRelease && cd ..",
    "android-key": "cd ./android/app/ && keytool -genkey -v -keystore debug.keystore -storepass android -alias androiddebugkey -keypass android -keyalg RSA -keysize 2048 -validity 10000",
    "clean": "rm -rf node_modules/ && rm yarn.lock && yarn cache clean && yarn install",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf ./ios/build && rm -rf ./ios/Pods/ && cd ios && pod install --repo-update && cd ..",
    "clean-all": "yarn clean && yarn clean-android && yarn clean-ios",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 8'",
    "ios-notch": "react-native run-ios --simulator 'iPhone 12'",
    "ios-promax": "react-native run-ios --simulator 'iPhone 12 Pro Max'",
    "ios-ipadpro": "react-native run-ios --simulator 'iPad Pro (12.9-inch) (4rd generation)'",
    "devmenu-android": "adb shell input keyevent 82"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^9.3.6",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.3",
    "axios": "^0.26.1",
    "lottie-ios": "3.5.0",
    "lottie-react-native": "^5.1.4",
    "react": "18.2.0",
    "react-native": "0.70.4",
    "react-native-fast-image": "^8.6.1",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-reanimated": "^2.12.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.4.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "redux-axios-middleware": "^4.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "@types/react-redux": "^7.1.23",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.73.3",
    "react-test-renderer": "18.2.0",
    "redux-devtools-extension": "^2.13.9",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
