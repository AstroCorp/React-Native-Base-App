{
  "name": "rnb",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "rn-link": "react-native link",
    "apk": "cd android && ./gradlew assembleRelease && cd ..",
    "android-key": "cd ./android/app/ && keytool -genkey -v -keystore debug.keystore -storepass android -alias androiddebugkey -keypass android -keyalg RSA -keysize 2048 -validity 10000",
    "clean": "rm -rf node_modules/ && rm yarn.lock && yarn cache clean && yarn install",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf ./ios/build && rm -rf ./ios/Pods/ && cd ios && pod install && cd ..",
    "clean-all": "yarn clean && yarn clean-android && yarn clean-ios",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 8'",
    "ios-notch": "react-native run-ios --simulator 'iPhone 12'",
    "ios-promax": "react-native run-ios --simulator 'iPhone 12 Pro Max'",
    "ios-ipadpro": "react-native run-ios --simulator 'iPad Pro (12.9-inch) (4rd generation)'",
    "nox-player": "adb connect 127.0.0.1:62001",
    "devmenu-android": "adb shell input keyevent 82"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.10",
    "@react-navigation/stack": "^5.12.8",
    "@react-navigation/drawer": "^5.11.4",
    "@react-navigation/native": "^5.8.10",
    "axios": "^0.21.1",
    "lottie-ios": "^3.1.9",
    "lottie-react-native": "^3.5.0",
    "react": "17.0.1",
    "react-native": "0.63.4",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^2.16.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-axios-middleware": "^4.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@babel/core": "^7.12.10",
    "@bam.tech/react-native-make": "^3.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.17.0",
    "metro-react-native-babel-preset": "^0.65.0",
    "jest": "^26.6.3",
    "react-native-rename": "^2.6.0",
    "react-native-rename-next": "^2.5.3",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
